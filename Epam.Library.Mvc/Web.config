    <compilation debug="true" targetFramework="4.6.2" />
    <httpRuntime targetFramework="4.5.2" />
    <authentication mode="Forms">
      <forms defaultUrl="~" loginUrl="~/User/Login" timeout="20"/>
    </authentication>
    <roleManager defaultProvider="my" enabled="true">
      <providers>
        <add name="my" type="Epam.Library.Mvc.Models.MyRoleProvider"/>
      </providers>
    </roleManager>
  </system.web>
  <runtime>
